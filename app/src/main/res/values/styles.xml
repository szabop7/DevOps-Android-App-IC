<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Title" parent="TextAppearance.AppCompat">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">20sp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
    </style>

    <style name="Normal_text" parent="TextAppearance.AppCompat">
        <item name="android:textColor">#646665</item>
        <item name="android:textSize">17sp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
    </style>

    <style name="Chip_text" parent="TextAppearance.AppCompat">
        <item name="android:textColor">#646665</item>
        <item name="android:textSize">5sp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
    </style>

    <style name="Product_text" parent="TextAppearance.AppCompat">
        <item name="android:textColor">#646665</item>
        <item name="android:textSize">17sp</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>

    <style name="Product_text_market_place" parent="TextAppearance.AppCompat">
        <item name="android:textColor">#646665</item>
        <item name="android:textSize">17sp</item>
        <item name="android:layout_marginStart">30dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_marginEnd">30dp</item>
    </style>

    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>

</resources>